{
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "jsxSingleQuote": false,
  "printWidth": 120,
  "plugins": ["prettier-plugin-import-sort"],
  "overrides": [
    {
      "files": ["*.js", "*.jsx"],
      "options": {
        "plugins": [["prettier-plugin-import-sort", { "sort": "true" }]]
      }
    }
  ]
}
